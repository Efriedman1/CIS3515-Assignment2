<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/enterPassword"
        android:layout_width="139dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="287dp" />

    <TextView
        android:id="@+id/tvInvalidPass2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/errorPassword"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="299dp" />

    <TextView
        android:id="@+id/tvEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tvEmail"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="138dp" />

    <EditText
        android:id="@+id/EnterEmail"
        android:layout_width="137dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="127dp" />

    <EditText
        android:id="@+id/EnterPassword"
        android:layout_width="139dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="220dp" />

    <TextView
        android:id="@+id/tvInvalidPass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/errorPassword"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="286dp"
        tools:layout_editor_absoluteY="232dp" />

    <TextView
        android:id="@+id/tvPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tvPassword"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="232dp" />

    <TextView
        android:id="@+id/tvPassword3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/confirmPass"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="299dp" />

    <TextView
        android:id="@+id/tvInvalidEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/errorEmail"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="138dp" />

    <Button
        android:id="@+id/btnSave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btnSave"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="402dp" />
</androidx.constraintlayout.widget.ConstraintLayout>